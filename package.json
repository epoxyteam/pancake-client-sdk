{
  "name": "pancake-pos-sdk",
  "version": "1.0.0",
  "description": "Official TypeScript SDK for Pancake POS API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs src",
    "prepack": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "pancake",
    "pos",
    "api",
    "sdk",
    "typescript",
    "ecommerce"
  ],
  "author": "Phong H.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/epoxyteam/pancake-client-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/epoxyteam/pancake-client-sdk/issues"
  },
  "homepage": "https://github.com/epoxyteam/pancake-client-sdk#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}